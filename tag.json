---
layout: null
---
{
    {% for tag in site.tags %}
    "{{ tag[0] }}": [
        {% for post in tag[1] %}
        {
            "title": {{ post.title | jsonify }},
            "url": "{{ post.url }}",
            "date": "{{ post.date | date: '%B %d, %Y' }}",
            "category": "{{ post.category }}"
        }{% unless forloop.last %},{% endunless %}
        {% endfor %}
    ]{% unless forloop.last %},{% endunless %}
    {% endfor %}
}
